collection.ArrayList implements List {

    Array<Int> items;

    init(Structure<Int> mc, Int size) {
        Reference<ArrayList<Int>> this this();
        Pointer<Array<Int>> thisItems (this items);
        Array<Int> items dynamic(mc size);
        thisItems copyFrom(items);
        Task t complete();
    }

    set(Int index, Int value) {
        Reference<ArrayList<Int>> this this();
        Pointer<Array<Int>> thisItems (this items);
        Array<Int> items;
        thisItems copyTo(items);
        ArrayAccessor<Int> p (items);
        p indexInt(index);
        p copyFrom(value);
        Task t complete();
    }

}
