test.StaticListTest {

    main() {
        AdminRef admin init();
        Reference<StaticList<Int>> staticList new();
        Structure<Int> mc ();
        Int size 5;
        staticList init(mc size);
        InterfaceReference<List<Int>> list (staticList);

        ~ Get the list accessor
        InterfaceReference<ListAccessor<Int>> listAccessor;
        Pointer<InterfaceReference<ListAccessor<Int>>> listAccessorPtr (listAccessor);
        list accessor(listAccessorPtr);

        ~ Add two elements
        Int elementOne 19;
        Pointer<Int> elementOnePtr (elementOne);
        listAccessor add (elementOnePtr);
        Int elementTwo 21;
        Pointer<Int> elementTwoPtr (elementTwo);
        listAccessor add (elementTwoPtr);

        ~ Iterator usage
        Int sum 0;
        ListIterator<Int> (list) {
            sum + element;
        };

        Exit;
    }

}