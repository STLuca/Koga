parser machineComposite;

structures {
    core.Int;
}

core.Optional<Structure T> {

    Byte[1] present;
    Byte[0] val;

    constructor() {
        i(ADD, II, P, present, I, 0d0, I, 0d0);
        Byte[G T] val;
    }

    constructor(T pVal) {
        i(ADD, II, P, present, I, 0d1, I, 0d0);
        Byte[G T] val;
        m(COPY, III, P, val, P, pVal, G, T);
    }

    get(Int p) {
        Proxy(p, val);
    }

}