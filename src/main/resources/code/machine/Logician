parser machineComposite;

structures {
    core.Int;
}

core.Logician {

    constructor schedule(Int objectAddr, Int tableAddr, Int methodAddr, Int taskAddr, Int adminTaskAddr) {
        logician(SET_OBJECT, T, P, objectAddr.val);
        logician(SET_TABLE, T, P, tableAddr.val);
        logician(SET_ALT_TASK, T, P, adminTaskAddr.val);
        logician(SET_METHOD_AND_TASK, TT, P, methodAddr.val, P, taskAddr.val);
    }

}