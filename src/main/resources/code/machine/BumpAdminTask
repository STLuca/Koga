parser machine;

core.BumpAdminTask {

    Byte[4] taskAddr;
    Byte[4] object;
    Byte[4] table;
    Byte[4] instruction;
    Byte[4] altTask;
    Byte[4] return;

    constructor loadInAdmin() {
        i(ADD, LI, LDA, taskAddr, R, altTask, IL, 0d0);
        i(ADD, LI, LDA, object, R, object, IL, 0d0);
        i(ADD, LI, LDA, table, R, table, IL, 0d0);
        i(ADD, LI, LDA, instruction, R, instruction, IL, 0d0);
        i(ADD, LI, LDA, altTask, R, task, IL, 0d0);
    }

}