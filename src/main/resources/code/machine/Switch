parser machineComposite;

structures {
    core.Int;
}

core.Switch {

    Addr jumps;
    Addr cases;
    Addr end;

    constructor(Int x) {
        j(REL, T, ADA, x.val);
        Addr jumps;
        Addr cases;
    }

    case(b8 label, Block block) {
        Addr case;
        Position jumps prev;
        j(REL, I, case);
        Position cases;
        Addr case;
        Block block;
        j(REL, I, end);
        Addr end;
        Position prev;
    }

}