parser machine;

Try {

    Addr handler;
    Addr end;

    constructor() {
    }

    constructor(Block block) {
        Block block;
        j(REL, I, end);
    }

    handle(Block block) {
        j(REL, I, end);
        Addr handler;
        Block block;
        Addr end;
    }

    throw() {
        j(REL, I, handler);
    }

    succeed() {
        j(REL, I, end);
    }

}

old {
    Addr handler;
    Addr end;

    constructor Try() {
        f(SET_CURR_HANDLER, handler);
    }

    constructor Try(Block block) {
        f(SET_CURR_HANDLER, handler);
        Block block;
    }

    handle(Block block) {
        j(REL, I, end);
        Addr handler;
        Block block;
        Addr end;
    }
}