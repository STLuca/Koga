parser host;

structures {
    core.Int;
    core.Exit;
    parser.ParserContext;
}

documents {
    util.SimpleAdministrator;
}

test.UnionTest {

    main() {
        Int x 1;
        ParserContext pcNum aNumber();
        pcNum match
            (Number) {
                Number addToFromNumber(x);
            }
            (Letter) {
                Letter addToFromLetter(x);
            };
        ParserContext pcLet aLetter();
        pcLet addTo(x);
        Exit;
    }

}
