parser machine;

Optional<Usable T> {

    Byte[1] present;
    Byte[0] val;

    constructor() {
        i(ADD, II, LDA, present, IL, 0d0, IL, 0d0);
        Byte[LG T] val;
    }

    constructor(T pVal) {
        i(ADD, II, LDA, present, IL, 0d1, IL, 0d0);
        Byte[LG T] val;
        m(COPY, III, LDA, val, ADA, pVal, LG, T);
    }

    get(Int p) {
        Proxy(p, val);
    }

}