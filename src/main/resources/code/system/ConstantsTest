parser host;

structures {
    core.Array;
    core.ArrayAccessor;
    core.Byte;
    core.String;
    core.Exit;
}

documents {
    util.SimpleAdministrator;
}

constants {
    array [10  32  67  127];
    hello "Hello World!";
}

test.ConstantsTest {

    main() {
        Array<Byte> b fromBytes(array);
        ArrayAccessor<Byte> p (b);
        Byte x 0;
        p index(2);
        p copyTo(x);

        String s (hello);
        Byte y 0;
        ArrayAccessor<Byte> sp (s);
        sp index(4);
        sp copyTo(y);

        Exit;
    }

}
