parser host;

usables {
    core.Int RenamedInt;
    core.Status;
    core.Exit;
}

documents {
    util.SimpleAdministrator;
}

test.EnumTest {

    main() {
        RenamedInt x 0;
        Status s (Error);
        s match(Success) {
            x + 2;
        } (Error) {
            x + 4;
        } (Waiting) {
            x + 6;
        };
        Exit;
    }

}