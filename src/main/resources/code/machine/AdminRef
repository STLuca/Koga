parser machineReference;

core.AdminRef {

    Byte[4] table;
    Byte[4] frame;

    constructor() {}

    constructor init() {
        Byte[4] methodAddr;
        Symbol(METHOD, methodSymbol, I, core.Administrator, I, init);
        c(ADDR, LI, methodAddr, table, methodSymbol);

        Byte[4] frameDataAddr;
        i(ADD, LI, LDA, frameDataAddr, R, altTask, I, 0d0);
        Args();

        logician(START_ADMIN, T, LDA, methodAddr);
    }

    invoke(Name methodName) {
        Byte[4] methodAddr;
        Symbol(METHOD, methodSymbol, I, core.Administrator, L, methodName);
        c(ADDR, LI, methodAddr, table, methodSymbol);

        Byte[4] frameDataAddr;
        i(ADD, LI, LDA, frameDataAddr, R, altTask, I, 0d0);
        Args();

        logician(START_ADMIN, T, LDA, methodAddr);
    }

    arg(Variable a, Name methodName) {
        m(COPY, TII, LDA, frameDataAddr, ADA, a, ADS, a);
        i(ADD, TI, LDA, frameDataAddr, LDA, frameDataAddr, ADS, a);
    }

}