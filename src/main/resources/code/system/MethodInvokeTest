parser host;

usables {
    core.Byte;
    core.Int;
    core.Pointer;
    core.Reference;
    core.Return;
    core.Exit;
    core.AdminRef;
    core.Task;
}

documents {
    util.SimpleAdministrator;
}

test.MethodInvokeTest {

    main() {
        AdminRef admin init();
        Int x 0;
        Int y 1;
        Pointer<Int> p (x);
        Reference<MethodInvokeTest> this this();
        this second(x);
        y + 1;
        Exit;
    }

    second(Pointer<Int> r) {
        Int a 5;
        r <- a;
        Task t complete();
    }

}